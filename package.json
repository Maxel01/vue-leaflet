{
  "name": "@vue-leaflet/vue-leaflet",
  "version": "0.0.0",
  "license": "MIT",
  "private": "false",
  "type": "module",
  "sideEffects": false,
  "main": "dist/vue-leaflet.cjs.js",
  "unpkg": "dist/vue-leaflet.umd.js",
  "module": "dist/vue-leaflet.es.js",
  "files": [
    "dist/"
  ],
  "types": "./dist/src/lib.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:vue-leaflet/vue-leaflet.git"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest --coverage",
    "build": "pnpm run type-check && pnpm run build-only",
    "build-only": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "release": "release-it"
  },
  "dependencies": {
    "leaflet": "^2.0.0-alpha",
    "prettier": "^3.6.2",
    "ts-debounce": "^4.0.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "peerDependencies": {
    "leaflet": "^2.0.0-alpha"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.0.14",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "^10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@release-it/conventional-changelog": "^8.0.2",
    "release-it": "^17.11.0"
  }
}
